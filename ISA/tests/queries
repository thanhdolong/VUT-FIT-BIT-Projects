#1 get all entries ending with 00
"uid=*00"

#2 get one entry xdolon00
"uid=xdolon00"

#3 get everything except xdolon00
'(!(uid=xdolon00))'

#4 get xdolik00, xdolon00 and xillic00
'(|(&(uid=xdosli00)(email=xdosli00@stud.fit.vutbr.cz))(uid=xdolon00)(&(|(|(uid=xkamen06)(uid=xillic00))(uid=xchova17))(!(uid=xchova17))(!(uid=xkamen06)))(uid=iaojsdjfso))'

#5 get max 5 results
-z 5 "uid=x*"

#6 find šveryone except xdolon00
'(!(|(&(cn=*Thanh)(uid=xdolon00))(&(cn=Tomas)(mail=xtomas0@email.cz))))'

